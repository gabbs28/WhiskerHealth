{
    "name": "whisker-health",
    "author": "Gabriela Ruiz",
    "version": "1.0.0",
    "private": true,
    "description": "## Navigation > [API Docs](https://github.com/gabbs28/WhiskerHealth/wiki)",
    "scripts": {
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "install": "npm --prefix prisma install prisma && npm --prefix backend install backend && npm --prefix frontend install frontend",
        "install:prisma": "npm --prefix prisma install prisma",
        "install:backend": "npm --prefix backend install backend",
        "install:frontend": "npm --prefix frontend install frontend",
        "start:backend": "npm run --prefix backend start:development",
        "start:frontend": "npm run --prefix frontend dev",
        "start:production": "npm run --prefix backend start:production",
        "build": "npm run --prefix prisma generate && npm run --prefix backend build && npm run --prefix frontend build",
        "build:prisma": "npm run --prefix prisma generate",
        "build:backend": "npm run --prefix backend build ",
        "build:frontend": "npm run --prefix frontend build",
        "database:migrate": "npm run --prefix prisma migrate",
        "database:seed": "npm run --prefix prisma seed"
    },
    "dependencies": {
        "@prisma/client": "^6.11.0",
        "bcryptjs": "^3.0.2",
        "json-with-bigint": "^3.4.4"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^24.0.10",
        "prettier": "^3.6.2",
        "prisma": "^6.11.0",
        "tsx": "^4.20.3"
    }
}
